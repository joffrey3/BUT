#include <stdio.h>
#include <stdlib.h>
#include <string.h>
int main(int argc,char** argv){
  int i=0,j=0, ent1=0,ent2=0,nombre,flag1=0,flag2=0;
  char car[1000],chaine[1000],liste[10]="0123456789";
  for(;argv[i]!=NULL;i++){
    strcpy(car,argv[i]);
    for(j=0;argv[i][j]!='\0';j++){
      chaine[i]=argv[i][j];
      nombre=nombre+1; 
    }
  }
  for(i=0;i<nombre;i++){
    printf("%s",chaine[nombre-i]);
  }
  printf("\n");
  return EXIT_SUCCESS;
}
